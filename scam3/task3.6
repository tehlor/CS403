(include "random.lib")
(randomSeed 86)
(define dq (deque))
(define (upback dq n)
    (define value (randomRange 10 100))
    (cond
        ((= n 0)
            ((dq'enqueueBack) value)
            )
        (else
            ((dq'enqueueBack) value)
            (upback dq (- n 1))
            )
        )
    )
(define (frontback dq n)
    (define (remove x mode)
        (cond
            ((= x n) (println "...]"))
            (else
                (print ((dq mode)) ",")
                (remove (+ x 1) mode)
                )
            )
        )
    ;((dq'display)) (println)
    (print "[")
    (remove 0 'dequeueFront)
    (print "[")
    (remove 0 'dequeueBack)
    )
(upback dq 3000)
(frontback dq 20)
