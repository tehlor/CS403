(include "random.lib")
(randomSeed 76)
(define dq (deque))
(define (upfront dq n)
    (define value (randomRange 10 100))
    (cond
        ((= n 0)
            ((dq'enqueueFront) value)
            )
        ((< (dq'size) 10)
            ((dq'enqueueFront) value)
            (upfront dq (- n 1))
            )
        (else
            ((dq'enqueueIndex) (randomRange 2 10) value)
            (upfront dq (- n 1))
            )
        )
    )
(define (frontback dq n)
    (define (remove x mode)
        (cond
            ((= x n) (println "...]"))
            (else
                (print ((dq mode)) ",")
                (remove (+ x 1) mode)
                )
            )
        )
    ;((dq'display)) (println)
    (print "[")
    (remove 0 'dequeueFront)
    (print "[")
    (remove 0 'dequeueBack)
    )
(upfront dq 3037)
(frontback dq 20)
